{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Read(**)",
      "WebFetch",
      "WebSearch",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(curl:*)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(mkdir:*)",
      "Bash(make:*)",
      "Bash(docker:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git add:*)",
      "Bash(git mv:*)",
      "Bash(git commit:*)",
      "Bash(git branch:*)",
      "Bash(git worktree:*)",
      "Bash(git checkout:*)",
      "Bash(git log:*)",
      "Bash(git fetch:*)",
      "Bash(git pull:*)",
      "Bash(git bisect:*)",
      "Bash(git -C:*)",
      "Bash(gh:*)",
      "Bash(npm run:*)",
      "Bash(npm search:*)",
      "Bash(npm show:*)",
      "Bash(npm test:*)",
      "Bash(npm install:*)",
      "Bash(npm i:*)",
      "Bash(npm install:*)",
      "Bash(npm ci:*)",
      "Bash(npm info:*)",
      "Bash(npx vitest:*)",
      "Bash(go:*)",
      "mcp__mcplex__*"
    ],
    "defaultMode": "acceptEdits",
    "additionalDirectories": [
      "~/src/witsy",
      "~/src/multi-llm-ts",
      "~/src/station1-playbook",
      "~/src/station1-desktop",
      "~/src/station1-server",
      "~/src/station1-project-server",
      "~/src/station1-llm-server",
      "~/src/station1-rrago",
      "~/src/station1-analytics",
      "~/src/ko-db-migrations",
      "~/src/ko-k8s-apps"
    ]
  },
  "model": "opus",
  "hooks": {},
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh",
    "padding": 0
  },
  "enabledPlugins": {
    "crew@nbonamy": true,
    "gopls-lsp@claude-plugins-official": true,
    "clangd-lsp@claude-plugins-official": true,
    "claude-crew": true,
    "swift-lsp@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": false
}
